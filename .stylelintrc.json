{
	"extends": "stylelint-config-standard",
	"plugins": [
		"./lib/stylelint-plugin/gameface.js"
	],
	"rules": {
		
		"GFP/gameface": true,
		"color-no-invalid-hex": true,
		"font-family-no-duplicate-names": true,
		"property-no-vendor-prefix": true,
		"value-no-vendor-prefix": true,
		"selector-max-universal": 1,
		"comment-empty-line-before": null,
		"comment-whitespace-inside": null,
		"selector-attribute-operator-allowed-list": [
			"=",
			"~=",
			"|=",
			"^=",
			"$=",
			"*="
		],
		"selector-combinator-allowed-list": [
			" ",
			">",
			"+",
			"~"
		],
		"selector-pseudo-class-allowed-list": [
			"hover",
			"active",
			"focus",
			"root",
			"first-child",
			"last-child",
			"only-child",
			"nth-child",
			"selection",
			"before",
			"after"
		],
		"selector-pseudo-element-allowed-list": [
			"selection",
			"before",
			"after"
		],
		"property-no-unknown": [
			true,
			{
				"ignoreProperties": [
					"text-stroke",
					"text-stroke-width",
					"selection-background-color",
					"selection-color",
					"text-stroke-color",
					"coh-font-fit-mode",
					"coh-font-fit-max-size",
					"coh-font-fit-min-size",
					"coh-font-sdf",
					"coh-custom-effect-name",
					"coh-custom-effect",
					"coh-custom-effect-string-param-1",
					"coh-custom-effect-string-param-2",
					"coh-composition-id",
					"coh-custom-effect-float-param-1",
					"coh-custom-effect-float-param-2",
					"coh-custom-effect-float-param-3",
					"coh-custom-effect-float-param-4",
					"coh-rendering-option",
					"coh-font-fit"
				]
			}
		],
		"property-allowed-list": [
